    def update_sub_domain 
        # byebug
        @client.update(slug: params[:sub_domain])
        if @client.update(sub_domain: params[:sub_domain]) 
            flash[:alert] = 'Sub Domain Updated'
            redirect_to articles_path(client_id: @client.sub_domain)
        end
    end
